<%= simple_form_for(@media_outlet) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :url %>
    <%= f.input :media_outlet_type_id,:collection => MediaOutletType.all,:label_method => :name,:value_method => :id,:label => "Type" ,:include_blank => false %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>